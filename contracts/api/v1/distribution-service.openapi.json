{
  "openapi": "3.0.3",
  "info": {
    "title": "Distribution Service API",
    "version": "v1"
  },
  "paths": {
    "/api/v1/distribution/items/{id}/overlays": {
      "post": {
        "summary": "Add overlay to distribution item"
      }
    },
    "/api/v1/distribution/items/{id}/preview": {
      "get": {
        "summary": "Preview distribution item"
      }
    },
    "/api/v1/distribution/items/{id}/schedule": {
      "post": {
        "summary": "Schedule distribution item for publishing"
      }
    },
    "/api/v1/distribution/items/{id}/reschedule": {
      "post": {
        "summary": "Reschedule previously scheduled item"
      }
    },
    "/api/v1/distribution/items/{id}/publish": {
      "post": {
        "summary": "Publish item immediately"
      }
    },
    "/api/v1/distribution/items/{id}/download": {
      "post": {
        "summary": "Generate downloadable export"
      }
    },
    "/api/v1/distribution/items/{id}/publish-multi": {
      "post": {
        "summary": "Publish item to multiple platforms"
      }
    },
    "/api/v1/distribution/items/{id}/retry": {
      "post": {
        "summary": "Retry failed distribution item"
      }
    }
  }
}
